        <item name="android:indeterminateDrawable">@drawable/nn_theme_scrubber_progress_horizontal_holo_dark</item>
        <item name="android:thumb">@drawable/nn_theme_scrubber_control_selector_holo_dark</item>
    </style>

    <style name="RatingBarnn_theme" parent="android:Widget.Holo.RatingBar">
