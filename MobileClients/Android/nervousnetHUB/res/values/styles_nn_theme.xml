<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AutoCompleteTextViewnn_theme" parent="android:Widget.Holo.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/nn_theme_list_selector_holo_dark</item>
        <item name="android:background">@drawable/nn_theme_edit_text_holo_dark</item>
    </style>

    <style name="Buttonnn_theme" parent="android:Widget.Holo.Button">
        <item name="android:background">@drawable/nn_theme_btn_default_holo_dark</item>
    </style>

    <style name="ImageButtonnn_theme" parent="android:Widget.Holo.ImageButton">
        <item name="android:background">@drawable/nn_theme_btn_default_holo_dark</item>
    </style>

    <style name="Spinnernn_theme" parent="android:Widget.Holo.Spinner">
        <item name="android:background">@drawable/nn_theme_spinner_background_holo_dark</item>
        <item name="android:dropDownSelector">@drawable/nn_theme_list_selector_holo_dark</item>
    </style>

    <style name="Tabnn_theme">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/nn_theme_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextnn_theme">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ProgressBarnn_theme" parent="android:Widget.Holo.ProgressBar.Horizontal">
